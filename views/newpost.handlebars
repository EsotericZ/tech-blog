{{> navbar}}

<div class="card">
  <h1 class="card-header">Create New Post</h1>
  <form class="card-body new-post-form" id="new-post-form">
    <label for="titleField" class="form-label">Title</label>
    <input type="text" class="form-control" id="titleField" aria-describedby="emailHelp">
    <label for="bodyField" class="form-label">Body</label>
    <input type="text" class="form-control" id="bodyField" aria-describedby="emailHelp">
    <button type="submit" class="btn btn-dark" id="addPostBtn">Submit</button>
  </form>
</div>